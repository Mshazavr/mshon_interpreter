fn fib(i) {
    imagine i - 0 {
        imagine i - 1 {
            checkit fib(i-1)+fib(i-2);
        }
        bummer {
            checkit 1;
        }
    }
    bummer {
        checkit 1;
    }
}

vomit fib(5);

